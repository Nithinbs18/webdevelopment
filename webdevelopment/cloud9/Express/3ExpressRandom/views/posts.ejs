<% include partials/header %>
<h1>The Posts Page</h1>

<% for(var i = 0; i < posts.length; i++){ %>
 <li>
     <%= posts[i].name %> - <strong><%= posts[i].age %></strong>
 </li>
<% } %>
<br/>
<br/>
<% posts.forEach(function(post){ %>
 <li>
     <%= post.name %> - <strong><%= post.age %></strong>
 </li>
<% }) %>
<% include partials/footer %>