<% include partials/header %>
<div class="container">
    <header class="jumbotron">
        <div class="container">
                <h1>Parks of Bangalore</h1>
                <h4>ಜೈ ಕರ್ನಾಟಕ...</h3>
                <p>
                     <a class="btn btn-primary" href="/parks/new">Click here to add a new park to the list.</a>
                </p>
        </div>
    </header>
    <div class="container">
        <div class="row">
                <% EJSparks.forEach(function(park){%>
                    <div class="col-md-3 col-sm-6">
                        <div class="img-thumbnail">
                                <img src="<%= park.image%>" height="150" width="230"/>
                            <div class="caption">
                                <p><strong><%= park.name %></strong></p>
                            </div>
                            <p>
                                <a href="/parks/<%= park._id %>" class="btn btn-primary pull-center" > More Information</a> 
                            </p>
                        </div>
                    </div>
                <% });%>
        </div>
    </div>
</div>

<% include partials/footer %>
